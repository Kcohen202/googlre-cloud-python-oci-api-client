[run]
branch = True
disable_warnings = module-not-measured

[report]
fail_under = 100
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
    # Ignore abstract methods
    raise NotImplementedError
omit =
  */gapic/*.py
  */proto/*.py
  */core/*.py
  */site-packages/*.py
  google/cloud/__init__.py
