[run]
branch = True

[report]
omit =
    */_generated/*.py
    */unit_tests/__init__.py
    # Packages in the "google.cloud" package that we don't own.
    */google/cloud/gapic/*
fail_under = 100
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
